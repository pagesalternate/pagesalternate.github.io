if (typeof window.Webcollage == 'undefined')
  window.Webcollage = {};

window.Webcollage._extForMinMax = new RegExp("(?:\\.min)?\\.(js|css)$", "i");

window.Webcollage.minOrMax = function(fileName) {
  if(document.location.search.indexOf("webcollage-scripts=max") == -1)
    return fileName.replace(window.Webcollage._extForMinMax, ".min.$1");
  else
    return fileName;
};

