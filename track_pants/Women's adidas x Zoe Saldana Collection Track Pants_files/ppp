var _wcscript = document.createElement('script');
_wcscript.setAttribute("type","text/javascript");

_wcscript.setAttribute("id","wc-loaded-acssite");
if (document.location.search.indexOf("wc-acs-libs=jit") >= 0) {
  window.Webcollage.wcAcsLibsParamIsJit = true;
  _wcscript.setAttribute("src", "https://content-preview.webcollage.net/aplus-partner-defs/kohls/resources/connections/aplus/js/acssite%40s.js?wc-acs-libs=jit");
} else {
  if (_wccontent.acssiteSig.indexOf("R") == 0) {
    var acsSiteUrl = Webcollage.minOrMax("https://scontent.webcollage.net/apps/pp/assets/kohls/js/acssite%40s.js");
    if (acsSiteUrl.lastIndexOf(".min.js") + 7 == acsSiteUrl.length)
        _wcscript.setAttribute("src",  acsSiteUrl + "?ver=" + _wccontent.minAcssiteSig);
    else
        _wcscript.setAttribute("src",  acsSiteUrl + "?ver=" + _wccontent.acssiteSig);
  } else
    _wcscript.setAttribute("src", Webcollage.minOrMax(_wccontent.contentgatewayurl + "/kohls/resources/connections/aplus/js/acssite%40s.js") + "?ver=2020.10.2.491");
}

